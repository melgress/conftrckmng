package main;
import java.io.*;

public class Ctm_main {

	public static void main(String[] args) {
		
	
		try {
			
			
			
			Reader reader =
				    new BufferedReader(
				        new FileReader("test3"));
			String currentline = ((BufferedReader) reader).readLine();
			String durationTalks = currentline.replaceAll("[^0-9]", "");

				 while (currentline  != durationTalks) {

					    System.out.println(currentline);
					    
					    System.out.println(durationTalks);
					   }
			
				Talks Hello = new Talks ("test", 2);
				Hello.testPrint();
		    

		    } catch (IOException e) {
		    	 System.out.println("An error occurred.");
				e.printStackTrace();
			}

	}

}

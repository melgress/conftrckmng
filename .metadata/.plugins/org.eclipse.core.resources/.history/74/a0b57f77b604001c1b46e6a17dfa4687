package main;
import java.io.*;

public class Ctm_main {

	public static void main(String[] args) {
		
	
		try {
			String currentline;
			Reader reader =
				    new BufferedReader(
				        new FileReader("test3"));

				 while ((currentline = ((BufferedReader) reader).readLine()) != null) {

					    System.out.println(currentline);
					   }
			
				Talks Hello = new Talks ("test", 2);
				Hello.testPrint();
		    

		    } catch (IOException e) {
		    	 System.out.println("An error occurred.");
				e.printStackTrace();
			}

	}

}
